
config dnsmasq
	option domainneeded '1'
	option localise_queries '1'
	option rebind_protection '1'
	option rebind_localhost '1'
	option local '/lan/'
	option domain 'lan'
	option expandhosts '1'
	option cachesize '1000'
        option noresolv '1'
        option server '8.8.8.8'
        option server '8.8.4.4'
        option server '1.1.1.1'
	option authoritative '1'
	option readethers '1'
	option leasefile '/tmp/dhcp.leases'
	option resolvfile '/tmp/resolv.conf.d/resolv.conf.auto'
	option localservice '1'
	option ednspacket_max '1232'
	list server '192.168.1.1'
        list address '/captive.apple.com/192.168.1.1'
        list address '/www.apple.com/library/test/success.html/192.168.1.1'
        list address '/www.msftconnecttest.com/192.168.1.1'
        list address '/connectivitycheck.gstatic.com/192.168.1.1'
        list address '/clients3.google.com/192.168.1.1'
        list address '/msftconnecttest.com/192.168.1.1'
	option serversfile '/captive.html'

config dhcp 'lan'
	option interface 'lan'
	option start '100'
	option limit '150'
	option leasetime '12h'
	option dhcpv4 'server'
	option dhcpv6 'server'
	option ra 'server'
	option ra_slaac '1'
	list ra_flags 'managed-config'
	list ra_flags 'other-config'

config dhcp 'wan'
	option interface 'wan'
	option ignore '1'

config odhcpd 'odhcpd'
	option maindhcp '0'
	option leasefile '/tmp/hosts/odhcpd'
	option leasetrigger '/usr/sbin/odhcpd-update'
	option loglevel '4'

